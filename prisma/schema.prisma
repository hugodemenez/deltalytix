datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
  schemas   = ["public"]
}
generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

model Trade {
  id             String @id
  accountNumber  String
  quantity       Int
  buyId          String
  sellId         String
  instrument     String
  buyPrice       String
  sellPrice      String
  buyDate        String
  sellDate       String
  pnl            String
  timeInPosition Float @default(0)
  userId         String
  side           String @default("")
  comission      Float @default(0)

  @@schema("public")
}
