// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Trade {
  id             BigInt @id @default(autoincrement())
  quantity       Int
  buyId          BigInt
  sellId         BigInt
  instrument     String
  buyPrice       String
  sellPrice      String
  buyDate        String
  sellDate       String
  pnl            String
  timeInPosition String
}
