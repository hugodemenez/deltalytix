generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
  schemas   = ["public"]
}

model Trade {
  id             String @id
  accountNumber  String
  quantity       Int
  buyId          BigInt
  sellId         BigInt
  instrument     String
  buyPrice       String
  sellPrice      String
  buyDate        String
  sellDate       String
  pnl            String
  timeInPosition String
  userId         String

  @@schema("public")
}
