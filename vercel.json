{
  "crons": [
    {
      "path": "/api/investing?lang=fr",
      "schedule": "0 6 * * 1"
    },
    {
      "path": "/api/investing?lang=en",
      "schedule": "0 6 * * 1"
    },
    {
      "path": "/api/cron/renewal-notice",
      "schedule": "0 9 * * *"
    }
  ],
    "redirects": [
      {
        "source": "/:path*",
        "destination": "https://deltalytix.app/business/:path*",
        "permanent": true,
        "has": [
          {
            "type": "host",
            "value": "business.deltalytix.app"
          }
        ]
      }
    ]
}